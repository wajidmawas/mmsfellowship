
@{

    ViewBag.Title = "CHAMPIONING YOUNG LEADERS FOR  A BETTER TOMORROW";
}
<script src="~/Scripts/ngStorage.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<style type="text/css">


    /* CSSTerm.com Simple CSS menu */
    body {
        background: #fff !important;
        overflow-y: auto;
    }

    .inner_div1 {
        height: auto;
    }
    #navbar, footer{
        display:none;
    }
    label {
        font-size: 14px;
        font-weight: 500;
        color: #414042;
    }
    .bdr_yellor {
        border-top: 1px solid #ffcd00;
        border-bottom: 1px solid #ffcd00;
    }
    .bdr-btm_yellow {
        border-bottom: 1px solid #ffcd00;
    }
    .btn{
        border-radius:10px;
    }
    .custom-file {
        position: relative;
        display: inline-block;
        width: auto;
        padding:10px 0
    }

        .custom-file input[type="file"] {
            position: absolute;
            width: auto;
            height: 100%;
            opacity: 0; /* Hide default input */
            cursor: pointer;
        }

    .custom-file-label {
        background: #e9ecf3; /* Change background color */
        color: #9e9e9e;
        padding: 5px 15px;
        text-align: center;
        border-radius: 5px;
        cursor: pointer;
        transition: 0.3s ease-in-out;
    }

        .custom-file-label:hover {
            background: #0056b3; /* Darker on hover */
        }

    .form-control, .form-select {
        background-color: #e8eaf1;
    }


    /* The snackbar - position it at the bottom and in the middle of the screen */
    #snackbar {
        visibility: hidden; /* Hidden by default. Visible on click */
        min-width: 250px; /* Set a default minimum width */
        margin-left: -125px; /* Divide value of min-width by 2 */
        background-color: #1caaec; /* Black background color */
        color: #fff; /* White text color */
        text-align: center; /* Centered text */
        border-radius: 2px; /* Rounded borders */
        padding: 16px; /* Padding */
        position: fixed; /* Sit on top of the screen */
        z-index: 9999; /* Add a z-index if needed */
        left: 50%; /* Center the snackbar */
        bottom: 30px; /* 30px from the bottom */
    }

        /* Show the snackbar when clicking on a button (class added with JavaScript) */
        #snackbar.show {
            visibility: visible; /* Show the snackbar */
            /* Add animation: Take 0.5 seconds to fade in and out the snackbar.
  However, delay the fade out process for 2.5 seconds */
            -webkit-animation: fadein 0.5s, fadeout 0.5s 2.5s;
            animation: fadein 0.5s, fadeout 0.5s 2.5s;
        }
    .msg {
        word-wrap: break-word;
        min-width: 50px;
        max-width: 90%;
        padding: 12px 20px;
        -webkit-border-radius: 20px;
        -moz-border-radius: 20px;
        border-radius: 20px;
        background: #1caaec;
        color: white;
        position: absolute;
        left: 0;
        right: 0;
        bottom: 0;
        height: 100px;
        width: 22%;
        margin: auto;
    }
    .loading {
        position: absolute;
        z-index: 9999;
        background: white;
        width: 100%;
        height: 100%;
        left: 0;
        right: 0;
        margin: auto;
        text-align: center;
        display: flex;
        justify-content: center;
        align-items: center;
    }
        .loading img {
            width: 150px;
        }
    .main_title img{
        width:300px
    }

    @@media screen and (max-width: 767px) {
        .main_title img {
            width: 150px;
        }
    }
</style>
<div ng-app="myApp" ng-controller="mmsfellowshipController">
    <section class="position-relative">

        <img src="~/Content/images/main_slider1.jpg" class="w-100 Mainslider" />
        <div class="container">
            <div class="main_title"> 
                <a href="/"><img src="~/Content/images/main_logo_white.png" /></a>
                <h1 class="text-white">
                    CHAMPIONING<br />
                    YOUNG LEADERS<br />
                    FOR A BETTER<br />
                    TOMORROW
                </h1>
               
            </div>
        </div>

    </section>
   
    <div class="container">
        <div class="main_left_panel">

            <div class="inner_div1 row mt-5"> 
                 
                <div class="col-md-6 mx-lg-auto"> 
                    <div class="row col-md-12 msg" id="content" style="display:none">
                        <p style="color:white">
                            <br /> 
                            <b>
                                Thank You for Sharing Your Voice!
                                <br />
                            </b>
                            <br />
                        </p>

                    </div>
                    <h3 class="mb-3 mt-5 title font-normal divRegister"><b>Register</b></h3>
                    <div class="row" id="divForm">
                        <div class="loading" style="display:none">
                            <img src="@Url.Content("~/Content/images/spinner.gif")" class="spinner" />
                        </div>
                        <div class="col-md-12">
                            <div class="row g-3 align-items-center mb-3">
                                <div class="col-lg-3">
                                    <label for="inputPassword6" class="col-form-label">Full Name *</label>
                                </div>
                                <div class="col-lg-9">
                                    <input type="text" id="txtName" ng-model="Name" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="row g-3 align-items-center mb-3">
                                <div class="col-lg-3">
                                    <label for="inputPassword6" class="col-form-label">DOB</label>
                                </div>
                                <div class="col-lg-9">
                                    <input type="date" id="txtDOB" ng-model="DOB" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="row g-3 align-items-center mb-3">
                                <div class="col-lg-3">
                                    <label for="inputPassword6" class="col-form-label">Gender *</label>
                                </div>
                                <div class="col-lg-9">
                                    <select class="form-select" id="drpGender" ng-model="Gender">
                                        <option value="">Select Gender</option>
                                        <option value="Male">Male</option>
                                        <option value="Female">Female</option>
                                        <option value="Other">Other</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="row g-3 align-items-center mb-3">
                                <div class="col-lg-3">
                                    <label for="inputPassword6" class="col-form-label">Mobile No *</label>
                                </div>
                                <div class="col-lg-9">
                                    <input type="text" id="txtMobileNo" ng-model="MobileNo" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="row g-3 align-items-center mb-3">
                                <div class="col-lg-3">
                                    <label for="inputPassword6" class="col-form-label">Email *</label>
                                </div>
                                <div class="col-lg-9">
                                    <input type="text" id="txtEmail" ng-model="EmailAddress" class="form-control" />
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="row g-3 align-items-center mb-3">
                                <div class="col-lg-3">
                                    <label for="inputPassword6" class="col-form-label">State *</label>
                                </div>
                                <div class="col-lg-9">
                                    <select class="form-select" id="drpStateList" ng-model="StateID" ng-change="FilterDistrict()">
                                        <option value="">Select State</option>
                                        <option value="{{x.STATEID}}" ng-repeat="x in StateList">{{x.STATENAME}}</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="row g-3 align-items-center mb-3">
                                <div class="col-lg-3">
                                    <label for="inputPassword6" class="col-form-label">District *</label>
                                </div>
                                <div class="col-lg-9">
                                    <select class="form-select" id="drpStateList" ng-model="District">
                                        <option value="">Select District</option>
                                        <option value="{{x.DistrictID}}" ng-repeat="x in DistrictList">{{x.DistrictName}}</option>
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="row g-3 align-items-center mb-3">
                                <div class="col-lg-3">
                                    <label for="inputPassword6" class="col-form-label">Qualification *</label>
                                </div>
                                <div class="col-lg-9">
                                    <select class="form-select" id="drpStateList" ng-model="Qualification">
                                        <option value="">Select Qualification </option>
                                        <option value="Graduate">Graduate</option>
                                        <option value="Post Graduate">Post Graduate</option>
                                        <option value="Others">Others</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 bdr_yellor mb-3 mt-3">
                            <div class="row g-3 align-items-center">
                                <div class="col-lg-4">
                                    <label for="inputPassword6" class="col-form-label">  Upload Aadhar card *</label>
                                </div>
                                <div class="col-lg-8">
                                    <div class="custom-file">
                                        <input type="file" id="txtFile_phot" accept="image/*" ng-model="Image">
                                        <label for="txtFile_phot" id="File_phot" class="custom-file-label">Choose File</label>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12 bdr-btm_yellow">
                            <div class="form-group d-flex flex-column input">
                                <label>
                                    Submit a video (not more than 2 mins) answering the following question,
                                    You may record the answer in the language of your choice
                                    <br /><br />
                                    <b>“What is one change you are most determined to bring to Indian politics, and why?”</b>*
                                </label>
                                <div class="custom-file">
                                    <input type="file" id="myFile" accept="video/mp4,video/x-m4v,video/*" ng-model="Video">
                                    <label for="txtFile" id="lblFile" class="custom-file-label">Choose File</label>
                                </div>

                            </div>
                        </div>
                        <div class="col-md-12" style="margin-top:5%;margin-bottom:5%">
                            <button class="btn btn-warning w-100 text-white" id="btnSubmit" ng-click="SaveRecord()">Submit</button>
                        </div>
                    </div>
                </div>
            </div>


        </div>

    </div>
</div>

<script src="~/Scripts/jquery-3.4.1.min.js"></script>

<script type="text/javascript"> 
    var pageId = '@ViewBag.pageId'
    var membercode = ''
    $(document).ready(function () {
        $(".left_main_panel").hide();
    })
</script>
